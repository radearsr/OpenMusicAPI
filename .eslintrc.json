{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": ["off", "windows"],
        "quotes": ["error", "double"],
        "no-underscore-dangle": ["error", {"allow": true}],
        "no-console": ["off"],
        "new-parens": ["off"],
        "camelcase": ["off"],
        "newline-per-chained-call": ["off"]
    }
}
